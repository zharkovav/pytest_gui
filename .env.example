# Pytest GUI Environment Variables Template
# Copy this file to .env and modify as needed

# Test execution settings
PYTEST_TIMEOUT=300
PYTEST_WORKERS=auto
PYTEST_VERBOSITY=1

# Application settings
LOG_LEVEL=INFO
THEME=default
AUTO_DISCOVER=true

# Test environment variables
TEST_DATABASE_URL=sqlite:///test.db
TEST_API_URL=http://localhost:8000
TEST_DEBUG=false

# Custom test markers
CUSTOM_MARKERS=performance,security,api

# Output settings
REPORT_FORMAT=html
REPORT_PATH=reports/